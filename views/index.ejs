    <script>
    $( document ).ready(function() {
      var data = {};
      data.store = 'Flipkart';
      data.action = 'deals';
      data.flag = 'deals'
      sessionStorage.setItem("store", data.store);
      sessionStorage.setItem("action", data.action);
      sessionStorage.setItem("flag", data.flag);
      sessionStorage.setItem("searchObject",'');
      getProducts(data);

      $('.store-menu').click(function(){
        $('.search-input').val("");
        $('.store-menu').removeClass('active');
        $(this).addClass('active');
        var action = $(this).attr('data-action');
        var store = $(this).attr('data-store');
        var title = $(this).attr('data-title');
        var flag = $(this).attr('data-flag');
        sessionStorage.setItem("store", store);
        sessionStorage.setItem("action",action);
        sessionStorage.setItem("flag",flag);
        sessionStorage.setItem("searchObject",'');
        data.title = title;
        data.store = store;
        data.action = action;
        data.flag = flag;
        getProducts(data);

      });

      $(".search-input").keyup(function(event) {
        if (event.keyCode === 13) {
          var input = $('.search-input').val();
          if(input == ''){
            if(data.flag=='category'){
              var storeLower = (data.store).toLowerCase()
              var searchUrl = $('#'+storeLower+'-category').val();
              var searchCategory = $('#'+storeLower+'-category :selected').attr('data-category');
              var datam={}
              datam.category = searchCategory;
              if(searchUrl!=""){
                switch (storeLower){
                  case 'flipkart' :
                      datam.url = searchUrl;
                      fkcategorySearch(datam);
                      break;
                  case 'amazon':
                      console.log("function implementation coming soooon");
                      break;
                }

              }else{
                swal("Select a gategory to search", "", "warning");
              }

            }else{
              data.store = sessionStorage.store;
              data.action = sessionStorage.action;
              data.flag = sessionStorage.flag;
              getProducts(data);
            }

          }else{
              $('.search-btn').click();
          }
        }
      });

      $('.search-btn').click(function(){
        var input = $('.search-input').val();
        if(input == ''){
          swal("Enter product name to serach", "", "warning");
        }else{
          var html='';
          var regex = new RegExp(input, "i");
          var searchResult = 0;
          if(data.flag =='deals' || data.flag =='offers'){
            var searchObject = JSON.parse(sessionStorage.searchObject);
            searchResult = 0;
    			  $.each(searchObject, function(key, val){
              var nameSearch = ((val.name==null || val.name=='null' || val.name==undefined || val.name=='undefined')) ? false : (val.name.search(regex) != -1);
              var titleSearch = ((val.title==null || val.title=='null' || val.title==undefined || val.title=='undefined')) ? false : (val.title.search(regex) != -1);
              var descSearch = ((val.description==null || val.description=='null' || val.description==undefined || val.description=='undefined')) ? false : (val.description.search(regex) != -1);
    				      if ((nameSearch) || (titleSearch) || (descSearch)) {
                    html += '<div class="col-lg-4 col-md-6 mb-4 product-holder"><div class="card h-100 product-img"><a target="_blank" href='+val.url+'><img class="card-img-top" src='+val.imageUrls[3].url+' alt=""></a>';
                    html += '<div class="card-body"><h4 class="card-title"><a target="_blank" href='+val.url+'>'+val.name+'</a></h4>';
                    html +=  '<h5>'+val.title+'</h5><p class="card-text">'+(val.description=='null' ? '' : val.description )+'</p></div></div></div>';
                    searchResult++;
                }
    			  });
          }else if(data.flag=='category'){

            if(data.store=='Flipkart'){
              var searchObject = JSON.parse(sessionStorage.searchObject);
              searchResult = 0;
      			  $.each(searchObject, function(key, val){
                var brandSearch = ((val.productBrand==null || val.productBrand=='null' || val.productBrand==undefined || val.productBrand=='undefined')) ? false : (val.productBrand.search(regex) != -1);
                var titleSearch = ((val.title==null || val.title=='null' || val.title==undefined || val.title=='undefined')) ? false : (val.title.search(regex) != -1);
                var descSearch = ((val.productDescription==null || val.productDescription=='null' || val.productDescription==undefined || val.productDescription=='undefined')) ? false : (val.productDescription.search(regex) != -1);
      				  if ((brandSearch) || (titleSearch) || (descSearch)) {

                  var images = JSON.stringify(val.imageUrls);
                  var images = JSON.parse(images);
                  html += '<div class="col-lg-4 col-md-6 mb-4 product-holder"><div class="card h-100 product-img"><a target="_blank" href='+val.productUrl+'><img class="card-img-top-search" src="'+images['400x400']+'" alt=""></a>';
                  html += '<div class="card-body"><h4 class="card-title"><a class="product-title" target="_blank" href='+val.productUrl+' title="'+val.title+'">'+val.title+'</a></h4>';
                  if(val.flipkartSellingPrice =='null' || val.flipkartSellingPrice==null ){
                    html +=  '<div style="float:left;margin-right:10px">'+val.maximumRetailPrice.currency+' '+val.maximumRetailPrice.amount+'</div></div></div></div>';
                  }else{
                    html +=  '<div style="float:left;margin-right:10px;color:green;font-weight:bold;">'+val.flipkartSellingPrice.currency+' '+val.flipkartSellingPrice.amount+'</div>';
                    html +=  '<div style="text-decoration:line-through;color:red;">'+val.maximumRetailPrice.currency+' '+val.maximumRetailPrice.amount+'</div></div></div></div>';
                  }
                  searchResult++;
                }
      			  });
            }

            if(data.store=='Amazon'){
              console.log("Search differs from others");
            }

          }else{
            data.keyword = input;
            searchAPI(data);
            searchResult = 0;
            return false;
          }

          if(searchResult >0){
            $('.produc-list-container').html(html);
          }else{
            $('.produc-list-container').html("No matching products found");
          }
        }
      });

      function fkcategorySearch(dataObject){
        console.log(dataObject);
            $.ajax({
              url : "/fk/search/category",
              type : "POST",
              data : dataObject,
              success : function(data) {

                if (data.flag) {
                    var response = data.response.products;
                    var categoryObject = [];
                    var html='';
                    for(var i=0;i<response.length;i++){
                      if(response[i].productBaseInfoV1.inStock){
                        categoryObject.push(response[i].productBaseInfoV1);
                        var images = JSON.stringify(response[i].productBaseInfoV1.imageUrls);
                        var images = JSON.parse(images);
                        html += '<div class="col-lg-4 col-md-6 mb-4 product-holder"><div class="card h-100 product-img"><a target="_blank" href='+response[i].productBaseInfoV1.productUrl+'><img class="card-img-top-search" src="'+images['400x400']+'" alt=""></a>';
                        html += '<div class="card-body"><h4 class="card-title"><a class="product-title" target="_blank" href='+response[i].productBaseInfoV1.productUrl+' title="'+response[i].productBaseInfoV1.title+'">'+response[i].productBaseInfoV1.title+'</a></h4>';
                        if(response[i].productBaseInfoV1.flipkartSellingPrice =='null' || response[i].productBaseInfoV1.flipkartSellingPrice==null ){
                          html +=  '<div style="float:left;margin-right:10px">'+response[i].productBaseInfoV1.maximumRetailPrice.currency+' '+response[i].productBaseInfoV1.maximumRetailPrice.amount+'</div></div></div></div>';

                        }else{
                          html +=  '<div style="float:left;margin-right:10px;color:green;font-weight:bold;">'+response[i].productBaseInfoV1.flipkartSellingPrice.currency+' '+response[i].productBaseInfoV1.flipkartSellingPrice.amount+'</div>';
                          html +=  '<div style="text-decoration:line-through;color:red;">'+response[i].productBaseInfoV1.maximumRetailPrice.currency+' '+response[i].productBaseInfoV1.maximumRetailPrice.amount+'</div></div></div></div>';

                        }
                      }
                    }
                    sessionStorage.setItem("searchObject", JSON.stringify(categoryObject));
                    $('.produc-list-container').html(html);
                } else {
                  $('.produc-list-container').html("Please refresh and try again");
                  alert("Something has went wrong / store not implemented");
                }
              },
              error : function() {
                alert("Something went wrong");
              }
            });
      }

      function amazoncategorySearch(dataObject){
        console.log(dataObject);
            $.ajax({
              url : "/amazon/search/category",
              type : "POST",
              data : dataObject,
              success : function(data) {

                if (data.flag) {
                    var response = data.response.products;
                    var categoryObject = [];
                    var html='';
                    for(var i=0;i<response.length;i++){
                      if(response[i].productBaseInfoV1.inStock){
                        categoryObject.push(response[i].productBaseInfoV1);
                        var images = JSON.stringify(response[i].productBaseInfoV1.imageUrls);
                        var images = JSON.parse(images);
                        html += '<div class="col-lg-4 col-md-6 mb-4 product-holder"><div class="card h-100 product-img"><a target="_blank" href='+response[i].productBaseInfoV1.productUrl+'><img class="card-img-top-search" src="'+images['400x400']+'" alt=""></a>';
                        html += '<div class="card-body"><h4 class="card-title"><a class="product-title" target="_blank" href='+response[i].productBaseInfoV1.productUrl+' title="'+response[i].productBaseInfoV1.title+'">'+response[i].productBaseInfoV1.title+'</a></h4>';
                        if(response[i].productBaseInfoV1.flipkartSellingPrice =='null' || response[i].productBaseInfoV1.flipkartSellingPrice==null ){
                          html +=  '<div style="float:left;margin-right:10px">'+response[i].productBaseInfoV1.maximumRetailPrice.currency+' '+response[i].productBaseInfoV1.maximumRetailPrice.amount+'</div></div></div></div>';

                        }else{
                          html +=  '<div style="float:left;margin-right:10px;color:green;font-weight:bold;">'+response[i].productBaseInfoV1.flipkartSellingPrice.currency+' '+response[i].productBaseInfoV1.flipkartSellingPrice.amount+'</div>';
                          html +=  '<div style="text-decoration:line-through;color:red;">'+response[i].productBaseInfoV1.maximumRetailPrice.currency+' '+response[i].productBaseInfoV1.maximumRetailPrice.amount+'</div></div></div></div>';

                        }
                      }
                    }
                    sessionStorage.setItem("searchObject", JSON.stringify(categoryObject));
                    $('.produc-list-container').html(html);
                } else {
                  $('.produc-list-container').html("Please refresh and try again");
                  alert("Something has went wrong / store not implemented");
                }
              },
              error : function() {
                alert("Something went wrong");
              }
            });
      }

      function searchAPI(datam){
        $.ajax({
          url : "/store/search",
          type : "POST",
          data : datam,
          success : function(data) {
            $('.cart-loader').css('display','none');
            $('.produc-list-container').show();
            if (data.flag) {
              var html='';
              var response = '';
              if(datam.action=='deals'){
                response = data.data.dotdList;
                response = response.reverse();
                $('.flipkart-deals-count').html(response.length);

              }else if(datam.action == 'offers'){
                response = data.data.allOffersList;
                $('.flipkart-offer-count').html(response.length);
              }else if(datam.action == 'keyword'){
                console.log(data.data);
                console.log("search response");
                return false;
              }
              sessionStorage.setItem("searchObject", JSON.stringify(response));
              for (var i = 0; i< response.length; i++) {
                  html += '<div class="col-lg-4 col-md-6 mb-4 product-holder"><div class="card h-100 product-img"><a target="_blank" href='+response[i].url+'><img class="card-img-top" src='+response[i].imageUrls[3].url+' alt=""></a>';
                  html += '<div class="card-body"><h4 class="card-title"><a target="_blank" href='+response[i].url+'>'+response[i].name+'</a></h4>';
                  html +=  '<h5>'+response[i].title+'</h5><p class="card-text">'+(response[i].description=='null' ? '' : response[i].description )+'</p></div></div></div>';
              }
              $('.produc-list-container').html(html);
            } else {
              $('.produc-list-container').html("Something has went wrong or store not implemented");
            }
          },
          error : function() {
            alert("Something went wrong");
          }
        });
      }
